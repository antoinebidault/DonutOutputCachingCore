<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageTags>asp.net, performance, speed, cache, caching</PackageTags>
    <Version>1.0.0</Version>
    <Authors>Antoine BIDAULT</Authors>
    <Description>ASP.NET Core middleware providing pure server-side output caching through a simple-to-use API</Description>
    <Copyright>Copyright Â© DonutOutputCachingCore </Copyright>
    <PackageId>DonutOutputCachingCore</PackageId>
    <Product>DonutOutputCachingCore</Product>
    <PackageIconUrl>https://raw.githubusercontent.com/madskristensen/WebEssentials.AspNetCore.OutputCaching/master/logo.png</PackageIconUrl>
    <PackageProjectUrl>https://github.com/antoinebidault/DonutOutputCachingCore</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/antoinebidault/DonutOutputCachingCore/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/antoinebidault/DonutOutputCachingCore</RepositoryUrl>
  </PropertyGroup>

  <PropertyGroup>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\DonutOutputCachingCore.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="HtmlAgilityPack" Version="1.6.2" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.0.0" />
  </ItemGroup>

</Project>
